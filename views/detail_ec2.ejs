<script>
    function query_ec2_status(time_point, host, monitor_item) {
        $.ajax({
            url: window.cmdbbackurl + "/v1/awsserver" + window.location.pathname + "/point_status",
            type: 'get',
            dataType: 'json',
            xhrFields: {
                withCredentials: true
            },
            crossDomain: true,
            data: {
                time_point: time_point,
                host_ip: host,
                monitor_item: monitor_item
            },
            success: (d) => {
                console.log(d)
                var chart = {
                    type: 'bar'
                };
                var title = {
                    text: host + "进程的" + monitor_item + "使用情况"
                };
                var subtitle = {
                    text: "top10进程排名"
                };
                var xAxis = {
                    categories: ['Process Name'],
                    title: {
                        text: null
                    }
                };
                var yAxis = {
                    min: 0,
                    title: {
                        text: monitor_item + "占用率",
                        align: 'high'
                    },
                    labels: {
                        overflow: 'justify'
                    }
                };
                var tooltip = {
                    valueSuffix: '% '
                };
                var plotOptions = {
                    bar: {
                        dataLabels: {
                            enabled: true
                        }
                    }
                };
                var legend = {
                    layout: 'vertical',
                    align: 'right',
                    verticalAlign: 'top',
                    x: -40,
                    y: 100,
                    floating: true,
                    borderWidth: 1,
                    backgroundColor: ((Highcharts.theme && Highcharts.theme.legendBackgroundColor) ||
                        '#FFFFFF'),
                    shadow: true
                };
                var credits = {
                    enabled: false
                };

                var series = [];
                for (var i = 0; i < d[1].length; i++) {
                    series[i] = {
                        name: d[1][i] + " " + d[0][i],
                        data: [d[2][i]]

                    }
                };

                var json = {};
                json.chart = chart;
                json.title = title;
                json.subtitle = subtitle;
                json.tooltip = tooltip;
                json.xAxis = xAxis;
                json.yAxis = yAxis;
                json.series = series;
                json.plotOptions = plotOptions;
                json.legend = legend;
                json.credits = credits;
                $('#container').highcharts(json);

            },
            error: function (error) {
                console.log(error)
            }
        })
    }

    var body = new Vue({
        'el': "#body",
        'data': {
            ec2Data: "",
            path: window.location.pathname,
            disk: ""
        },
        mounted: function () {
            $.ajax({
                url: window.cmdbbackurl + '/v1/awsserver' + window.location.pathname,
                type: 'get',
                dataType: 'json',
                xhrFields: {
                    withCredentials: true
                },
                crossDomain: true,
                success: (data) => {
                    this.ec2Data = data
                },
                error: function (error) {
                    console.log(error)
                }
            });
            $.ajax({
                url: window.cmdbbackurl + '/v1/awsserver' + window.location.pathname + "/disk",
                type: 'get',
                dataType: 'json',
                xhrFields: {
                    withCredentials: true
                },
                crossDomain: true,
                success: (data) => {
                    this.disk = data
                },
                error: function (error) {
                    console.log(error)
                }
            });
            $.ajax({
                url: window.cmdbbackurl + "/v1/awsserver" + window.location.pathname + "/" +
                    'cpu',
                type: 'get',
                dataType: 'json',
                data: {
                    end_time: "1h",
                    polymerization: "Average",
                    period: "5m"
                },
                xhrFields: {
                    withCredentials: true
                },
                crossDomain: true,
                success: (data) => {
                    const json = getDataSource(data)
                    $('#ec2_' + 'cpu' + '_status').highcharts(json);
                    function getDataSource(args) {
                        var chart = {
                            type: 'spline'
                        };
                        var title = {
                            text: "服务器" + args[0]["monitor_item"] +
                                "使用状态"
                        };
                        var subtitle = {
                            text: args[0]["monitor_item"]
                        };
                        var xAxis = {
                            categories: args[0]["value"][(args[0]["value"].length -
                                1)]
                        };
                        var yAxis = {
                            title: {
                                text: args[0]["monitor_item"] + "使用率"
                            },
                            labels: {
                                formatter: function () {
                                    return this.value + '\xB0';
                                }
                            },
                            lineWidth: 2
                        };
                        var tooltip = {
                            crosshairs: true,
                            shared: true
                        };
                        var plotOptions = {
                            spline: {
                                marker: {
                                    radius: 4,
                                    lineColor: '#666666',
                                    lineWidth: 1
                                }
                            },
                            series: {
                                cursor: 'pointer',
                                events: {
                                    click: function (e) {
                                        $('#process_model').modal(
                                            'show')
                                        query_ec2_status(e.point.category,
                                            e.point.series.name,
                                            json.subtitle.text);
                                        // $('#process_model').modal('show')
                                        // alert(e.point.category);
                                    }
                                }
                            },
                        };
                        var series = [];
                        if (args[0]["monitor_item"] == "cpu") {
                            for (var i = 0; i < (args.length); i++) {
                                series[i] = {
                                    name: args[i]["host"] + ":" + args[i][
                                        "value"
                                    ][2]["quota"],
                                    marker: {
                                        symbol: 'square'
                                    },
                                    data: args[i]["value"][2]["value"]
                                }
                            }
                        } else if (args[0]["monitor_item"] == "memery") {
                            for (var i = 0; i < (args.length); i++) {
                                series[i] = {
                                    name: args[i]["host"] + ":" + args[i][
                                        "value"
                                    ][1]["quota"],
                                    marker: {
                                        symbol: 'square'
                                    },
                                    data: args[i]["value"][1]["value"]
                                }
                            }
                        }
                        var json = {};
                        json.chart = chart;
                        json.title = title;
                        json.subtitle = subtitle;
                        json.tooltip = tooltip;
                        json.xAxis = xAxis;
                        json.yAxis = yAxis;
                        json.series = series;
                        json.plotOptions = plotOptions;

                        return json
                    }
                },
                error: function (error) {
                    console.log(error)
                }
            });
            $.ajax({
                url: window.cmdbbackurl + "/v1/awsserver" + window.location.pathname + "/" +
                    'memery',
                type: 'get',
                dataType: 'json',
                data: {
                    end_time: "1h",
                    polymerization: "Average",
                    period: "5m"
                },
                xhrFields: {
                    withCredentials: true
                },
                crossDomain: true,
                success: (data) => {
                    const json = getDataSource(data)
                    $('#ec2_' + 'memery' + '_status').highcharts(json);
                    function getDataSource(args) {
                        var chart = {
                            type: 'spline'
                        };
                        var title = {
                            text: "服务器" + args[0]["monitor_item"] +
                                "使用状态"
                        };
                        var subtitle = {
                            text: args[0]["monitor_item"]
                        };
                        var xAxis = {
                            categories: args[0]["value"][(args[0]["value"].length -
                                1)]
                        };
                        var yAxis = {
                            title: {
                                text: args[0]["monitor_item"] + "使用情况"
                            },
                            labels: {
                                formatter: function () {
                                    return this.value + '\xB0';
                                }
                            },
                            lineWidth: 2
                        };
                        var tooltip = {
                            crosshairs: true,
                            shared: true
                        };
                        var plotOptions = {
                            spline: {
                                marker: {
                                    radius: 4,
                                    lineColor: '#666666',
                                    lineWidth: 1
                                }
                            },
                            series: {
                                cursor: 'pointer',
                                events: {
                                    click: function (e) {
                                        $('#process_model').modal(
                                            'show')
                                        query_ec2_status(e.point.category,
                                            e.point.series.name,
                                            json.subtitle.text);
                                        // $('#process_model').modal('show')
                                        // alert(e.point.category);
                                    }
                                }
                            },
                        };
                        var series = [];
                        if (args[0]["monitor_item"] == "cpu") {
                            for (var i = 0; i < (args.length); i++) {
                                series[i] = {
                                    name: args[i]["host"] + ":" + args[i][
                                        "value"
                                    ][2]["quota"],
                                    marker: {
                                        symbol: 'square'
                                    },
                                    data: args[i]["value"][2]["value"]
                                }
                            }
                        } else if (args[0]["monitor_item"] == "memery") {
                            for (var i = 0; i < (args.length); i++) {
                                series[i] = {
                                    name: args[i]["host"] + ":" + args[i][
                                        "value"
                                    ][1]["quota"],
                                    marker: {
                                        symbol: 'square'
                                    },
                                    data: args[i]["value"][1]["value"]
                                }
                            }
                        }
                        var json = {};
                        json.chart = chart;
                        json.title = title;
                        json.subtitle = subtitle;
                        json.tooltip = tooltip;
                        json.xAxis = xAxis;
                        json.yAxis = yAxis;
                        json.series = series;
                        json.plotOptions = plotOptions;

                        return json
                    }
                },
                error: function (error) {
                    console.log(error)
                }
            });
        },
        methods: {
            ec2_status_query: function (event) {
                var monitor_item = event.target.value
                $.ajax({
                    url: window.cmdbbackurl + "/v1/awsserver" + window.location.pathname +
                        "/" + monitor_item,
                    type: 'get',
                    dataType: 'json',
                    data: $('#ec2_' + monitor_item + '_status_form').serialize(),
                    xhrFields: {
                        withCredentials: true
                    },
                    crossDomain: true,
                    success: (data) => {
                        const json = getDataSource(data)
                        $('#ec2_' + monitor_item + '_status').highcharts(json);

                        function getDataSource(args) {
                            var chart = {
                                type: 'spline'
                            };
                            var title = {
                                text: "此业务各服务器" + args[0]["monitor_item"] +
                                    "使用状态"
                            };
