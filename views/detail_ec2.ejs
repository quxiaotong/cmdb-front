                            json.tooltip = tooltip;
                            json.xAxis = xAxis;
                            json.yAxis = yAxis;
                            json.series = series;
                            json.plotOptions = plotOptions;

                            return json
                        }
                    },
                    error: function (error) {
                        console.log(error)
                    }
                })
            }

        }
    })

</script>

